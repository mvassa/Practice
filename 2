#include "pch.h"
#include <math.h>
#include <iostream>
using namespace std;

int main()
{
	double n, m;
	double a = 4.8;
	double b = -7.9;
  
	if (a >= b)
	{
	  n = pow(a - b, 1.0 / 3);
	}
  
	if (a < b)
	{
		n = a * a + (a - b) / sin(a*b);
	}
  
	if (n == b)
	{
		m = n * n + tan(n*a);
	}
  
	if (n > b)
	{
		m = b * b * b + n * a * a;
	}
  
	if (n < b)
	{
		m = ((n + a) / -b) + sqrt(sin(a)*sin(a) - cos(n));
	}
  
	cout << "a = " << a << endl;
	cout << "b = " << b << endl;
	cout << "n = " << n << endl;
	cout << "m = " << m << endl;
  
	system("pause");
	return 0;
}
